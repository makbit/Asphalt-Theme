	"more_info_here": "https://code.visualstudio.com/api/language-extensions/semantic-highlight-guide",
	"token_info_press": "Ctrl+shift+P -> enter",
	"semanticHighlighting": true,

	
"tokenColors":
[
	{ "name": "Other",       "scope":[ "source", "support" ],                         "settings": { "foreground": "#FF0" } },
	{ "name": "Comment",     "scope":[ "comment", "punctuation.definition.comment" ], "settings": { "foreground": "#777" } },
	{ "name": "Doxygen",     "scope": "storage.type.class.doxygen",                   "settings": { "foreground": "#741" } },
	{ "name": "Invalid",     "scope": "invalid",                                      "settings": { "foreground": "#E15" } },
	{ "name": "Format%d",    "scope": "constant.other.placeholder",                   "settings": { "foreground": "#084" } },
	{ "name": "Enum value",  "scope": "variable.other.enummember",        "settings": { "foreground": "#08F" } },
	{ "name": "Global var",  "scope": "variable.other.global",                        "settings": { "ZZfontStyle": "bold" } },
	{ "name": "Label",       "scope": "entity.name.label",                            "settings": { "fontStyle": "bold", "foreground": "#F40" } },

	{
	"name": "Constant Number",
	"scope":[
		"constant",
		"constant.numeric",
		"support.constant",
		"keyword.other.unit",
		"entity.name.type.parameter"
		],
	"settings": { "foreground": "#F08" }
	},

	{
	"name": "Constant True/False",
	"scope":[
		"constant.language",
		"constant.other"
		],
	"settings": { "foreground": "#F0F" }
	},
	
	{
	"name": "String, CSS & JSON String",
	"scope":[
		"string",
		"punctuation.definition.string",
		"constant.character",
		"constant.other.object.key",
		"constant.other.symbol.quoted",
		"meta.object-literal.key",
		"variable.parameter.url"
		],
	"settings": { "foreground": "#480" }
	},

	{
	"name": "Variable",
	"scope":[
		"variable",
		"variable.name",
		"variable.function",
		"variable.language",
		"variable.other",
		"variable.other.local",
		"meta.definition.variable"
		],
	"settings": { "foreground": "#FE2" }
	},

	{
	"name": "Arguments (Variable)",
	"scope":[
		"variable.parameter",
		"variable.parameter.preprocessor",
		"constant.name.attribute.tag",
		"support.variable.property",
		"meta.function.argument",
		"meta.function-call.arguments"
		],
	"settings": { "foreground": "#FA2" }
	},


	{
	"name": "Class/member variable",
	"scope":[
		"variable.other.class",
		"variable.other.property",
		"meta.member.access",
		"meta.attribute",
		"meta.attribute.python",
		"meta.property.class",
		"meta.property.object",
		"meta.parameter.optional"
		],
	"settings": { "foreground": "#CF2" }
	},
	

	{
	"name": "Function",
	"scope":[
		"entity.name.function",
		"entity.name.function.definition",
		"entity.name.function.call",
		"support.function",
		"meta.function.lua",
		"meta.function.type-declaration",
		"meta.function-call",
		"meta.function-call.generic",
		"meta.method",
		"meta.method-call",
		"meta.method.declaration",
		"variable.language.super.ts"
		],
	"settings": { "foreground": "#4F0" }
	},

	{ "name": "Class member-function", "scope":[ "entity.name.function.member" ], "settings": { "foreground": "#0F4" } },

	{
	"name": "Storage modifier (extern, friend, this, &)",
	"scope":[
		"storage",
		"storage.modifier",
		"storage.modifier.reference",
		"variable.language.this",
		"variable.language.special.self",
		"variable.parameter.function.language.special.self",
		],
	"settings": { "foreground": "#FFF" }
	},

	{
	"name": "Storage type (int, char)",
	"scope":[
		"storage.type",
		"meta.qualified_type",
		"meta.template"
		],
	"settings": { "foreground": "#FFF" }
	},
	
	{
	"name": "Type (class/namespace) name",
	"scope":[
		"entity.name.class",
		"entity.name.type",
		"entity.name.namespace",
		"entity.name.tag",
		"entity.name.section",
		"entity.other.inherited-class",
		"support.type",
		"support.class",
		"support.variable"
		],
	"settings": { "foreground": "#FFF" }
	},

	
	{
	"name": "Preprocessor: (#include, #define)",
	"scope":[
		"keyword.control.directive",
		"keyword.control.import",
		"keyword.other.import",
		"keyword.control.at-rule",
		"meta.preprocessor",
		"meta.tag.preprocessor",
		"meta.import.elm",
		"punctuation.definition.directive",
		"variable.control.import.include"
	],
	"settings": { "fontStyle": "regular", "foreground": "#FFF" }
	},

	{
	"name": "Preprocessor: (Macro code)",
	"scope":[
		"entity.name.function.preprocessor",
		"meta.preprocessor.macro"
	],
				"settings":       { "foreground": "#0DA" }
	},
	
	{
	"name": "Punctuation",
	"scope":[
		"punctuation",
		"keyword.other.colon",
		"keyword.other.period",
		"begin.bracket",
		"end.bracket",
		"meta.brace",
		"meta.tag.xml",
		"meta.tag.template.block.twig",
		"meta.tag.template.value.twig",
		"meta.expression.parenthesized",
		"meta.structure.dictionary",
		"support.function.construct.begin",
		"support.function.construct.end"
		],
	"settings": { "foreground": "#FFF" }
	},

	{
	"name": "Keyword, Operator, Control",
	"scope":[
		"keyword.control",
		"keyword.function",
		"keyword.local",
		"keyword.operator",
		"keyword.other",
		"keyword.type",
		"keyword.type-alias",
		"keyword.blade",
		"keyword.begin",
		"keyword.end",
		"entity.name.function.operator",
		"support.class.console",
		"punctuation.definition.block.tag",
		"punctuation.definition.attribute"
		],
	"settings": { "foreground": "#FFF" }
	},

	{ "name": "Keyword-Control", "scope":[ "keyword.control" ], "settings": { "fontStyle": "bold" } },



	{ "name": "HTML Text",      "scope":[ "text.html.derivative", "text.html" ], "settings": { "foreground": "#EEE" } },
	{ "name": "HTML Tag name",  "scope":["entity.name.tag.html", "meta.tag"], "settings": { "foreground": "#08F" } },
	{ "name": "HTML Attribute", "scope":["entity.other.attribute-name", "support.type.property-name"], "settings": { "foreground": "#FE0" } },
	{ "name": "CSS Values",     "scope":[ "source.css", "meta.embedded" ], "settings": { "foreground": "#4A0" } },

	{ "scope": "meta.selector",     "settings": { "foreground": "#4F0" } },
	{ "scope": "token.info-token",  "settings": { "foreground": "#08F" } },
	{ "scope": "token.warn-token",  "settings": { "foreground": "#FE0" } },
	{ "scope": "token.error-token", "settings": { "foreground": "#F08" } },
	{ "scope": "token.debug-token", "settings": { "foreground": "#F40" } }
  ]
}
