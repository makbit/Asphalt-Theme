  "token_info_press": "Ctrl+shift+P -> enter",


"tokenColors": [
	{
	"name": "Comment",
	"scope": "comment, comment.block, punctuation.definition.comment",
	"settings": {
		"foreground": "#777"
		}
	},
	{
	"name": "Doxygen Keyword",
	"scope": "storage.type.class.doxygen",
	"settings": {
		"foreground": "#782"
		}
	},
	{
	"name": "Punctuation",
	"scope": "punctuation.section, punctuation.separator, punctuation.terminator, punctuation.definition.arguments, punctuation.definition.tag, punctuation.definition.capture, meta.brace, meta.structure.dictionary",
	"settings": {
		"foreground": "#FFF"
		}
	},
	{
	"name": "Keyword, Operator",
	"scope": "keyword.other, keyword.operator, storage.modifier.reference, keyword.other.namespace, storage.type.namespace",
	"settings": {
		"fontStyle": "normal",
		"foreground": "#FFF"
		}
	},
	{
	"name": "Control",
	"scope": "keyword.control",
	"settings": {
		"fontStyle": "bold",
		"foreground": "#FFF"
		}
	},



	{
	"name": "Namespace",
	"scope": "entity.name.namespace",
	"settings": {
		"fontStyle": "bold",
		"foreground": "#FFF" // white // "#8CF" cyan
		}
	},
	{
	"name": "Storage modifier (extern, friend, const)",
	"scope": "storage.modifier",
	"settings": {
		"fontStyle": "normal",
		"foreground": "#FFF" // white
		}
	},
	{
	"name": "Storage Type (int/class/enum)",
	"scope": "storage.type",
	"settings": {
		"fontStyle": "normal",
		"foreground": "#FFF" // white
		}
	},
	{
	"name": "Type (class/struct/enum) name",
	"scope": "entity.name.type, entity.name.type.class",
	"settings": {
		"fontStyle": "normal",
		"foreground": "#FFF" // "#4FF" cyan   "#4AF" sky
		}
	},

	

	{
	"name": "Number",
	"scope": "constant, constant.numeric, keyword.other.unit",
	"settings": {
		"foreground": "#F0F" // magenta "#F0F"
		}
	},
	{
	"name": "Constant",
	"scope": "constant.language, constant.other, support.constant, entity.name.type.parameter",
	"settings": {
		"fontStyle": "normal",
		"foreground": "#F0F" // "#F40" orange // "#82F" purple // "#C05" malina+
		}
	},



	{
	"name": "Enum value",
	"scope": "variable.other.enummember",
	"settings": {
		"foreground": "#1AB" // light cyan
		}
	},
	{
	"name": "String",
	"scope": "string, string.unquoted.label, constant.character, constant.other.object.key, meta.object-literal.key",
	"settings": {
		"foreground": "#282" // mid green
		}
	},
	{
	"name": "Preprocessor: (#include, #define), meta.preprocessor, meta.preprocessor.include",
	"scope": "keyword.control.directive, punctuation.definition.directive",
	"settings": {
		"foreground": "#14A" // dark blue  "#82F" // purple 
		}
	},
	{
	"name": "Preprocessor: (Macro code), meta.preprocessor.macro",
	"scope": "entity.name.function.preprocessor",
	"settings": {
		"foreground": "#28F" // light blue
		}
	},
	{
	"name": "Label",
	"scope": "entity.name.label",
	"settings": {
		"fontStyle": "bold",
		"foreground": "#F40" // orange  // "#82F" violet
		}
	},

	
		
	{
	"name": "Function",
	"scope": "entity.name.function.definition, entity.name.function.call, support.function, meta.function-call.generic",
	"settings": {
		"foreground": "#3E2" // green  "#248" // deep blue
		}
	},
	{
	"name": "Class member-function",
	"scope": "entity.name.function.member",
	"settings": {
		"foreground": "#2F8" //emerald
		}
	},
	{
	"name": "Class operator",
	"scope": "entity.name.function.operator",
	"settings": {
		"foreground": "#FFF" //white
		}
	},
	
	

	{
	"name": "Other",
	"scope": "source, variable",
	"settings": {
		"foreground": "#4AF" ///!!!!!  "#8BF" check
		}
	},


	{
	"name": "Variable",
	"scope": "variable.language, variable.function, variable.other, support.variable.property, meta.parens",
	"settings": {
		"foreground": "#FF0" // yellow
		}
	},
	{
	"name": "Variable Global",
	"scope": "variable.other.global",
	"settings": {
		"fontStyle": "bold",
		"foreground": "#FF0" // yellow  "#DF8" sand
		}
	},
	{
	"name": "Class (member) variable",
	"scope": "variable.other.class, variable.other.property, meta.property.class, meta.property.object, meta.parameter.optional",
	"settings": {
		"foreground": "#DF4" // lemon
		}
	},
	{
	"name": "Function arg",
	"scope": "variable.parameter, variable.parameter.preprocessor, meta.function.argument, meta.function-call.arguments",
	"settings": {
		"foreground": "#FA4" // mandarin  "#C80" // dark yellow
		}
	},







	
	{
	"name": "HTML Tag name",
	"scope": "entity.name.tag",
	"settings": {
		"foreground": "#82F" // purple "#1AB" // mid cyan
		}
	},
	{
	"name": "HTML Attribute name",
	"scope": "entity.other.attribute-name, meta.attribute, support.type.property-name",
	"settings": {
		"foreground": "#FD0" // yellow "#28F" light blue
		}
	},
	{
	"name": "HTML Text",
	"scope": "text.html",
	"settings": {
		"foreground": "#EEE" // "#ED0" sand-yellow
		}
	},
	{
	"name": "CSS & JSON String",
	"scope": "string.quoted.double",
	"settings": {
		"foreground": "#3B3" // forest
		}
	},





    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    }
  ]
}
